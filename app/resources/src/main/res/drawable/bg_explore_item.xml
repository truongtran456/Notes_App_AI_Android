<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:top="@dimen/dp_1">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dp_16" />
            <solid android:color="#D6A024" />

        </shape>

    </item>

    <item android:bottom="@dimen/dp_1">
        <shape>
            <corners android:radius="@dimen/dp_16" />
            <solid android:color="#FFCE5D" />
            <padding
                android:bottom="3.5dp"
                android:left="2dp"
                android:right="2dp"
                android:top="2dp" />
        </shape>
    </item>

    <item
        android:top="@dimen/dp_2"
        android:bottom="@dimen/dp_2"
        android:left="@dimen/dp_2"
        android:right="@dimen/dp_2">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dp_14" />
            <solid android:color="@android:color/white" />
        </shape>
    </item>

</layer-list>
