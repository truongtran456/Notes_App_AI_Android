<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <corners android:radius="@dimen/dp_8" />
            <gradient android:startColor="#FFC340" android:endColor="#FF8400" android:angle="-90"/>
        </shape>
    </item>

    <item android:top="@dimen/dp_2" android:bottom="@dimen/dp_2" android:start="@dimen/dp_2" android:end="@dimen/dp_2">
        <shape android:shape="oval">
            <solid android:color="?attr/primaryBackgroundColor"/>
        </shape>
    </item>
</layer-list>
